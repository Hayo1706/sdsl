top block Activations {
    stepnum = column "Step number": Int;
    description = column "Description": Str;
    motor1 ?= column "Motor1": Check;
    motor2 ?= column "Motor2": Check;
    heater1 ?= column "Heater1": Check;
    valve1 ?= column "Valve1": Check;
}
syntax Int = [0-9]+;
syntax Str = [\"]![\"]* [\"];
syntax Check = "x" | "X";